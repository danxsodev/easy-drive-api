{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "testEnvironment": "node",
  "coverageDirectory": "./coverage",
  "collectCoverageFrom": [
    "src/**/*.ts"
  ],
  "verbose": true
}